{
  "name": "kjs",
  "version": "1.0.0",
  "description": "Javascript in Korean",
  "main": "index.js",
  "dependencies": {
    "comment-regex": "^1.0.1",
    "kpop": "^0.1.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.5",
    "@babel/preset-env": "^7.1.5",
    "@babel/register": "^7.0.0",
    "@types/jest": "^23.3.10",
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "gulp": "^4.0.0",
    "gulp-tslint": "^8.1.3",
    "gulp-typedoc": "^2.2.1",
    "gulp-typescript": "^5.0.0-alpha.3",
    "gulp-typescript-formatter": "^0.1.3",
    "husky": "^1.2.0",
    "jest": "^23.6.0",
    "npm-run-all": "^4.1.3",
    "onchange": "^5.1.0",
    "prettier": "^1.15.1",
    "ts-jest": "^23.10.5",
    "tslint": "^5.11.0",
    "tslint-config-standard": "^8.0.1",
    "typedoc": "^0.13.0",
    "typescript": "^3.2.2",
    "typescript-formatter": "^7.2.2"
  },
  "scripts": {
    "test": "jest",
    "prettier:watch": "onchange '**/*.js' -- prettier --write {{changed}}",
    "eslint": "./node_modules/.bin/eslint src",
    "tslint": "./node_modules/.bin/tslint --fix --project ./tsconfig.json",
    "tslint:watch": "onchange '**/*.ts' -- npm run tslint",
    "format": "./node_modules/.bin/tsfmt -r",
    "format:watch": "onchange '**/*.ts' -- npm run format",
    "jsdoc": "./node_modules/.bin/jsdoc src -r -c ./conf.json -d docs",
    "babel": "babel src --out-dir dist",
    "babel:watch": "onchange '**/*.ts' -- npm run babel",
    "typescript": "./node_modules/.bin/tsc",
    "typedoc": "./node_modules/.bin/typedoc --out ./docs ./src",
    "watch": "onchange '**/*.ts' -- run-s format tslint typescript"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true
  },
  "keywords": [],
  "author": "Joel Mun",
  "license": "MIT"
}
